<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="app.admin.security.apikey_authenticator" class="Admin\Security\Firewall\ApiKeyAuthenticator">
            <argument type="service">
                <service class="Doctrine\ORM\EntityRepository" factory-service="doctrine.orm.entity_manager" factory-method="getRepository">
                    <argument>AdminBundle:ApiKey</argument>
                </service>
            </argument>
        </service>
    </services>
</container>
