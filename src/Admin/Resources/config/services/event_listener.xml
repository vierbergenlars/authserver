<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="app.admin.event_listener.clone_view" class="Admin\EventListener\CloneViewAnnotationListener">
            <tag name="kernel.event_subscriber" />
        </service>
        <service class="Admin\EventListener\AdminMenuListener">
            <argument type="service" id="security.authorization_checker" />
            <tag name="kernel.event_subscriber" />
        </service>
        <service class="Admin\EventListener\DefaultFilterListListener">
            <argument type="service" id="request_stack" />
            <tag name="kernel.event_subscriber" />
        </service>
        <service class="Admin\EventListener\DefaultUserDisplayListListener">
            <tag name="kernel.event_subscriber" />
        </service>
        <service class="Admin\EventListener\DefaultGroupDisplayListListener">
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
