{% if item %}
    {{ icon('envelope fa-fw') }} {{ item.email }}<br>
    <a href="{{ url('admin_user_get', {user: item.user.guid}) }}" title="{{ item.user.displayName }}">{{ icon('user fa-fw') }} {{ item.user.username }}</a>
{% else %}
    <span class="text-muted">{{ icon('envelope fa-fw') }}</span>
{% endif %}